{
  "name": "vue-input-otp",
  "type": "module",
  "version": "0.0.7",
  "packageManager": "pnpm@8.15.3",
  "description": "",
  "author": "Robert Soriano <sorianorobertc@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/wobsoriano/vue-input-otp#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wobsoriano/vue-input-otp.git"
  },
  "bugs": "https://github.com/wobsoriano/vue-input-otp/issues",
  "keywords": [],
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./*"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup --onSuccess \"pnpm build:dts\"",
    "build:dts": "vue-tsc --declaration --emitDeclarationOnly",
    "dev": "tsup --watch --onSuccess \"pnpm build:dts && pnpm --filter website dev\"",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "release": "bumpp && npm publish",
    "prepublishOnly": "pnpm build"
  },
  "peerDependencies": {
    "vue": "^3.2.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.4",
    "@vue/tsconfig": "^0.5.1",
    "bumpp": "^9.3.0",
    "eslint": "^8.56.0",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3",
    "unplugin-vue": "^5.0.3",
    "vue": "^3.4.19",
    "vue-tsc": "^1.8.27"
  }
}
